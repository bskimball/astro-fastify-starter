{
  "name": "astro-fastify-starter",
  "type": "module",
  "version": "0.0.3",
  "scripts": {
    "dev:astro": "astro dev",
    "dev:fastify": "tsx watch src/api/dev.ts",
    "dev": "NODE_ENV=development run-p dev:*",
    "build:astro": "astro build",
    "build:fastify": "rollup --config rollup-server.config.mjs --configPlugin typescript",
    "build:check": "tsc",
    "build": "NODE_ENV=production run-s build:astro build:fastify build:check",
    "start": "NODE_ENV=production node run-server.mjs",
    "astro": "astro",
    "migrate": "drizzle-kit migrate --config=drizzle.config.mjs",
    "generate": "drizzle-kit generate --config=drizzle.config.mjs",
    "studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.0.0",
    "@astrojs/react": "^4.0.0",
    "@astrojs/tailwind": "^5.1.3",
    "@fastify/cookie": "^11.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/middie": "^9.0.2",
    "@fastify/static": "^8.0.3",
    "@fastify/websocket": "^11.0.1",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@trpc/client": "^11.0.0-rc.449",
    "@trpc/server": "^11.0.0-rc.449",
    "astro": "^5.0.3",
    "better-sqlite3": "^11.6.0",
    "cookie": "^1.0.2",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.29.1",
    "drizzle-orm": "^0.37.0",
    "fastify": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^19.0.0",
    "react-aria-components": "^1.5.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "sharp": "^0.33.5",
    "superjson": "^2.2.2",
    "tailwindcss": "^3.4.16",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "rollup": "^4.28.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
