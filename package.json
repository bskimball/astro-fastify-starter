{
  "name": "astro-fastify-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev:astro": "astro dev",
    "dev:fastify": "tsx watch ./src/api/dev.ts",
    "dev": "NODE_ENV=development run-p dev:*",
    "build:astro": "astro build",
    "build:fastify": "rollup --config rollup-server.config.mjs --configPlugin typescript",
    "build:check": "tsc",
    "build": "run-s build:astro build:fastify build:check",
    "start": "node ./run-server.mjs",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/node": "^8.3.2",
    "@astrojs/react": "^3.6.0",
    "@astrojs/tailwind": "^5.1.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/middie": "^8.3.1",
    "@fastify/static": "^7.0.4",
    "@fastify/websocket": "^10.0.1",
    "@nextui-org/react": "^2.4.2",
    "@trpc/client": "^11.0.0-rc.449",
    "@trpc/server": "^11.0.0-rc.449",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.11.5",
    "fastify": "^4.28.1",
    "framer-motion": "^11.2.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4",
    "superjson": "^2.2.1",
    "tailwindcss": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.10",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "rollup": "^4.18.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
