<script>
  import { client } from '../api/client.ts'

  client.randomNumber.subscribe(undefined, {
    onData(data) {
      let el = document.getElementById('holder')

      if (el) {
        el.innerText = data.randomNumber.toString()
      }
    },
  })
</script>

<div>
  <div id="holder">0</div>
</div>
